module Config

context-free syntax

  Config.JavaBackend = <
    backend : Java
      <Phase?>
  >
  
  Phase.Generate = <phase : generate>
  Phase.Compile  = <phase : compile>
  Phase.Execute  = <phase : execute>
  
  Config.WebDSLBackend = <
    backend : WebDSL
      <UI?>
  >
  
  UI.None = <ui : None>
