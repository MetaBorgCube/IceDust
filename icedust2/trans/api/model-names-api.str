rules // attributes

  is-attributename = where(is-string;string-get-prop(|"Member","ns");?Attribute())

  attributename-is-derivation = where(is-string;string-get-prop(|"Member","derivationtype");?Derivation())
  attributename-is-default    = where(is-string;string-get-prop(|"Member","derivationtype");?DefaultValue())
  attributename-is-normal     = where(is-string;string-get-prop(|"Member","derivationtype");?Normal())
  
  attributename-is-normal-default     = where(attributename-is-normal  <+ attributename-is-default)
  attributename-is-default-derivation = where(attributename-is-default <+ attributename-is-derivation)
  
