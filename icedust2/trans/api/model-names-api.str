rules // entity

  is-entityname = where(is-string;string-get-prop(|"Entity","ns");?Entity())

  entityname-get-attributenames  = string-get-assoc-scope(|"Entity");scope-get-decls                ;filter(is-attributename);string-sort-anno
  entityname-get-attributenames2 = string-get-assoc-scope(|"Entity");scope-get-decls                ;filter(is-attributename);origin-sort-anno
  entityname-get-relationnames   = string-get-assoc-scope(|"Entity");scope-get-reachables(|"Member");filter(is-relationname );string-sort-anno
  entityname-get-rolenames       = string-get-assoc-scope(|"Entity");scope-get-decls                ;filter(is-rolename     );string-sort-anno
  entityname-get-inversenames    = string-get-assoc-scope(|"Entity");scope-get-reachables(|"Member");filter(is-inversename  );string-sort-anno
  entityname-get-shortcutnames   = string-get-assoc-scope(|"Entity");scope-get-reachables(|"Member");filter(is-shortcutname );string-sort-anno

