module names

imports
	
	include/Relations
	
namespaces
	
	Module
	Entity
	Property
	
rules

	Module(x, _) : 
		defines Module x
		scopes Entity
		
	Entity(x, _) :
		defines Entity x of type Type(x)
		scopes Property
		
	Property(x, t) :
		defines Property x of type t
		
	Type(x) :
		refers to Entity x
		refers to Entity "String"
		refers to Entity "Int"
