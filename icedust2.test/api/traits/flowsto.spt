test Query attribute flows to with type filter[[
module test

model

  entity Picture {
    dummy : String 1
    [[path]]: String 1
  }
  
  trait HasPicture {
  
  }
  
  relation Picture.usedBy <-> 1 HasPicture.picture
  
  entity Page extends HasPicture {
    display : String = "<img href='"+picture.path+"'>"
