module types-data2

language icedust2

fixture [[
  module test
  [[...]]
]]

test no errors on within multiplicity bounds [[
model

  entity Foo{}
  entity Bar{}
  relation Foo 1 <-> + Bar
  
data
  
  bar:Bar{}
  foo1:Foo{bar=bar}
  foo2:Foo{bar=bar}
  foo3:Foo{bar=bar}
]] 0 errors

test error on violating multiplicity bounds with inverse assignments [[
model

  entity Foo{}
  entity Bar{}
  relation Foo 1 <-> ? Bar
  
data
  
  [[bar]]:Bar{}
  foo1:Foo{bar=bar}
  foo2:Foo{bar=bar}
  foo3:Foo{bar=bar}
]] 1 error at #1
