module names-inlining

language icedust2

test Error on overriding inline [[
module test
model

  entity Foo {
    a : Int = 42 (inline)
  }
  
  entity Bar extends Foo {
    [[a]] : Int = 21 * 2
  }
]] 1 error at #1

test Error on overriding with inline [[
module test
model

  entity Foo {
    a : Int = 42
  }
  
  entity Bar extends Foo {
    [[a]] : Int = 21 * 2 (inline)
  }
]] 1 error at #1

test Error on inline cycle [[
module test
model

  entity Foo {
    a : Int = 42
    [[b : Int = 5 + a + b (inline)]]
  }
  
]] >= 1 error at #1

test Error on inline cycle 2 [[
module test
model

  entity Foo {
    [[a : Int = b (inline)]]
    [[b : Int = a (inline)]]
  }
  
]] >= 1 error at #1
   >= 1 error at #2