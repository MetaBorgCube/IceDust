module types-references-entities

language IceDust

fixture [[
	module test
	[[...]]
]]


test type of ref entity this [[
	model
	entity X{
		i : Int = [[this]].i (default)
	}
	
	data
	x : X {
		i=8
	}
]] run get-type on #1 to "X"
	 
test type of ref entity Identifier [[
	model
	entity X{
		i : Int = [[x]].i (default)
	}
	
	data
	x : X {
		i=6
	}
]] run get-type on #1 to "X"


test multiplicity of ref this [[
	model
	entity X{
		i : Int = [[this]].i (default)
	}
	
	data
	x : X {
		i=8
	}
]] run get-multiplicity on #1 to One()
	 
